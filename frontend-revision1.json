{
    "taskDefinitionArn": "arn:aws:ecs:ap-south-1:490004620091:task-definition/Frontend:1",
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "490004620091.dkr.ecr.ap-south-1.amazonaws.com/frontend:latest",
            "cpu": "256", 
            "memory": "512",
            "portMappings": [
                {
                    "name": "frontend-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/Frontend",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "family": "Frontend",
    "executionRoleArn": "arn:aws:iam::490004620091:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "revision": 1,
    "volumes": []
}
